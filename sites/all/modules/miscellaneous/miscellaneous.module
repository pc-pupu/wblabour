	$query = db_select('a_service_info', 'asi'); 
	$query ->fields('asi', array('service_id','sservice_cadre','s_doj','s_promotion_doj','s_dob','s_gender','s_uid','s_status','s_confirmdate','s_recruitmenttype','s_forwardstatus'));
